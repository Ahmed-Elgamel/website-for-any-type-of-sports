
@{
    ViewData["Title"] = "LogIn";
    Layout = "_LayoutSAM";
}

<h1>Sports Association Manager</h1>

@if (TempData["SignUp"]!=null)
{
    <h2>@TempData["SignUp"]</h2>
}



<div class="d-grid gap-2">
    <a asp-action="addMatch"  class="btn btn-outline-dark">ADD MATCH</a>
    <a asp-action="deleteMatch" class="btn btn-outline-danger">DELETE MATCH</a>
    <a asp-action="ViewAllUpcomingMatches"  class="btn btn-outline-primary">VIEW ALL UPCOMING MATCHES</a>
    <a asp-action="ViewPlayedMatches"  class="btn btn-outline-primary">VIEW ALL PLAYED MATCHES</a>
    <a asp-action="ViewPairsofClubsNeverMatched"  class="btn btn-outline-primary">VIEW CLUB PAIRS NEVER SCHEDULED TO PLAY EACH OTHER</a>

    @*<a asp-action="ViewPairsofClubsNeverMatched" asp-route="" class="btn btn-outline-primary">VIEW CLUB PAIRS NEVER SCHEDULED TO PLAY EACH OTHER</a>
    *@

</div>
